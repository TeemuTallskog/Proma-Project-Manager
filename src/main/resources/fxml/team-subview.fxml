<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="780.0" prefWidth="870.0" stylesheets="@../styling/time-management-styling.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="r8.view.mainView.projectView.subview.TeamSubviewController">
    <children>
        <HBox prefHeight="780.0" prefWidth="870.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <VBox id="container-vbox" prefHeight="780.0" prefWidth="410.0">
                    <children>
                        <Text fill="#000000b2" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Teams to Project">
                            <font>
                                <Font name="Arial Bold" size="16.0" />
                            </font>
                            <VBox.margin>
                                <Insets bottom="20.0" left="25.0" right="20.0" top="20.0" />
                            </VBox.margin>
                        </Text>
                        <Text fill="#000000b2" smooth="false" strokeType="OUTSIDE" strokeWidth="0.0" text="Team name">
                            <font>
                                <Font size="15.0" />
                            </font>
                            <VBox.margin>
                                <Insets left="30.0" />
                            </VBox.margin>
                        </Text>
                        <TextField id="taskName" fx:id="textFieldTeamName" minHeight="30.0" prefHeight="30.0">
                            <opaqueInsets>
                                <Insets />
                            </opaqueInsets>
                            <VBox.margin>
                                <Insets left="30.0" right="30.0" top="5.0" />
                            </VBox.margin>
                        </TextField>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="410.0">
                            <children>
                                <Button fx:id="btnAddTeam" mnemonicParsing="false" prefWidth="130.0" text=" Add" textFill="#f8f6f6">
                                    <font>
                                        <Font size="16.0" />
                                    </font>
                                </Button>
                            </children>
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                        </HBox>
                        <Text fill="#000000b2" smooth="false" strokeType="OUTSIDE" strokeWidth="0.0" text="Teams to be created">
                            <font>
                                <Font size="15.0" />
                            </font>
                            <VBox.margin>
                                <Insets bottom="5.0" left="30.0" />
                            </VBox.margin>
                        </Text>
                        <ListView fx:id="listViewTeamsToAdd" prefHeight="276.0" prefWidth="350.0">
                            <VBox.margin>
                                <Insets left="30.0" right="30.0" />
                            </VBox.margin>
                        </ListView>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Button fx:id="btnRemoveTeam" mnemonicParsing="false" prefWidth="130.0" text="Remove" textFill="#f8f6f6">
                                    <font>
                                        <Font size="16.0" />
                                    </font>
                                </Button>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Button fx:id="buttonCreateTeams" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Create Teams" textFill="#f8f6f6">
                                    <font>
                                        <Font size="16.0" />
                                    </font>
                                </Button>
                            </children>
                            <VBox.margin>
                                <Insets top="30.0" />
                            </VBox.margin>
                        </HBox>
                    </children>
                    <HBox.margin>
                        <Insets left="25.0" />
                    </HBox.margin>
                </VBox>
                <VBox id="container-vbox" prefHeight="780.0" prefWidth="410.0">
                    <children>
                        <Text fill="#000000b2" strokeType="OUTSIDE" strokeWidth="0.0" text="Existing Teams">
                            <font>
                                <Font name="Arial Bold" size="16.0" />
                            </font>
                            <VBox.margin>
                                <Insets bottom="20.0" left="25.0" right="20.0" top="20.0" />
                            </VBox.margin>
                        </Text>
                        <ListView fx:id="listViewExistingTeams" prefHeight="300.0" prefWidth="350.0">
                            <VBox.margin>
                                <Insets left="20.0" right="20.0" />
                            </VBox.margin>
                        </ListView>
                        <Text fill="#000000b2" strokeType="OUTSIDE" strokeWidth="0.0" text="Team Info">
                            <font>
                                <Font name="Arial Bold" size="16.0" />
                            </font>
                            <VBox.margin>
                                <Insets bottom="20.0" left="25.0" right="20.0" top="20.0" />
                            </VBox.margin>
                        </Text>
                    </children>
                    <HBox.margin>
                        <Insets left="25.0" right="25.0" />
                    </HBox.margin>
                </VBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
